{
    "manifest_version": 2,
    "name": "DuckDuckLib",
    "description": "Search DuckDuckGo with safe search turned OFF so you can judge what is offensive for yourself.",
    "version": "1.0.2",
    "omnibox": {"keyword": "ddl"},
    "icons": {
        "16": "icons/dax-logo-lib-16.png",
        "48": "icons/dax-logo-lib-48.png",
        "128": "icons/dax-logo-lib-128.png"
    },
    "browser_specific_settings": {
	"gecko": {
	    "strict_min_version": "65.0"
	}
    },
    "chrome_settings_overrides": {
	"search_provider": {
	    "name": "DuckDuckLib",
		"search_url": "https://duckduckgo.com/?q={searchTerms}&kl=wt-wt&kp=-2",
		"suggest_url": "https://duckduckgo.com/ac/?q={searchTerms}&kl=wt-wt&type=list",
	    "favicon_url": "https://duckduckgo.com/favicon.ico",
	    "is_default": true,
	    "encoding": "UTF-8"
	}
    }
}
